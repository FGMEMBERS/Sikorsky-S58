<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>tach.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>rotorneedle</object-name>
    <object-name>turbineneedle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>rotorneedle</object-name>
    <object-name>turbineneedle</object-name>
    <emission>
      <red>   0.028 </red>
      <green> 0.014 </green>
      <blue>  0.007 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>turbineneedle</object-name>
    <property>/sim/model/s58/engine</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  35 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 267 </dep></entry>
      <entry><ind> 1.4 </ind><dep> 360 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rotorneedle</object-name>
    <property>/rotors/main/rpm</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  35 </dep></entry>
      <entry><ind> 117 </ind><dep> 151 </dep></entry>
      <entry><ind> 235 </ind><dep> 267 </dep></entry>
      <entry><ind> 329 </ind><dep> 360 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
    </axis>
  </animation>

</PropertyList>

