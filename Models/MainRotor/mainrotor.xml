<?xml version="1.0"?>

<PropertyList>

  <path>mainrotor.ac</path>

  <animation>
    <name>rotorcomplet</name>
    <object-name>rotorup</object-name>
    <object-name>rotor</object-name>
    <object-name>pale1</object-name>
    <object-name>pale2</object-name>
    <object-name>pale3</object-name>
    <object-name>pale4</object-name>
    <object-name>pale1B</object-name>
    <object-name>pale2B</object-name>
    <object-name>pale3B</object-name>
    <object-name>pale4B</object-name>
    <object-name>axe1</object-name>
    <object-name>axe2</object-name>
    <object-name>axe3</object-name>
    <object-name>axe4</object-name>

    <!-- Transparence -->
    <object-name>propblur1</object-name>
    <object-name>propblur2</object-name>
    <object-name>propblur3</object-name>
    <object-name>propblur4</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propblur4B</object-name>
    <object-name>propdisc1</object-name>
    <object-name>propdisc2</object-name>
    <object-name>propdisc3</object-name>
    <object-name>propdisc4</object-name>
    <object-name>propdisc1B</object-name>
    <object-name>propdisc2B</object-name>
    <object-name>propdisc3B</object-name>
    <object-name>propdisc4B</object-name>
  </animation>

  <animation>
    <name>rotorpartiel</name>
    <object-name>rotorup</object-name>
    <object-name>pale1</object-name>
    <object-name>propblur1</object-name>
    <object-name>propdisc1</object-name>
    <object-name>pale2</object-name>
    <object-name>propblur2</object-name>
    <object-name>propdisc2</object-name>
    <object-name>pale3</object-name>
    <object-name>propblur3</object-name>
    <object-name>propdisc3</object-name>
    <object-name>pale4</object-name>
    <object-name>propblur4</object-name>
    <object-name>propdisc4</object-name>
    <object-name>pale1B</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propdisc1B</object-name>
    <object-name>pale2B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propdisc2B</object-name>
    <object-name>pale3B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propdisc3B</object-name>
    <object-name>pale4B</object-name>
    <object-name>propblur4B</object-name>
    <object-name>propdisc4B</object-name>
  </animation>   

  <animation>
    <type>noshadow</type>
    <object-name>propblur1</object-name>
    <object-name>propblur2</object-name>
    <object-name>propblur3</object-name>
    <object-name>propblur4</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propblur4B</object-name>
    <object-name>propdisc1</object-name>
    <object-name>propdisc2</object-name>
    <object-name>propdisc3</object-name>
    <object-name>propdisc4</object-name>
    <object-name>propdisc1B</object-name>
    <object-name>propdisc2B</object-name>
    <object-name>propdisc3B</object-name>
    <object-name>propdisc4B</object-name>
  </animation>

  <!-- Sélection Pâles/blur  rotor principal -->
  <animation>
    <type>select</type>
    <object-name>propdisc1</object-name>
    <object-name>propdisc2</object-name>
    <object-name>propdisc3</object-name>
    <object-name>propdisc4</object-name>
    <object-name>propdisc1B</object-name>
    <object-name>propdisc2B</object-name>
    <object-name>propdisc3B</object-name>
    <object-name>propdisc4B</object-name>
    <condition>
      <greater-than>
        <property>rotors/main/rpm</property>
        <value>150</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur1</object-name>
    <object-name>propblur2</object-name>
    <object-name>propblur3</object-name>
    <object-name>propblur4</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propblur4B</object-name>
    <condition>
      <and>
        <greater-than>
          <property>rotors/main/rpm</property>
          <value>60</value>
        </greater-than>
        <less-than>
          <property>rotors/main/rpm</property>
          <value>150.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>pale1</object-name>
    <object-name>pale2</object-name>
    <object-name>pale3</object-name>
    <object-name>pale4</object-name>
    <object-name>pale1B</object-name>
    <object-name>pale2B</object-name>
    <object-name>pale3B</object-name>
    <object-name>pale4B</object-name>
    <condition>
      <less-than>
        <property>rotors/main/rpm</property>
	<value>150</value>
      </less-than>
    </condition>
  </animation>

  <!-- Déformation du bout des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>pale1B</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propdisc1B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep>  4.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m>  5.165 </x-m>
       <y-m> -0.014 </y-m>
       <z-m>  0.079 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale2B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propdisc2B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep>  4.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> -0.014 </x-m>
       <y-m> -5.165 </y-m>
       <z-m>  0.079 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale3B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propdisc3B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>   0.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>   4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> -5.165 </x-m>
       <y-m>  0.015 </y-m>
       <z-m>  0.079 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale4B</object-name>
    <object-name>propblur4B</object-name>
    <object-name>propdisc4B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>   0.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>   4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> 0.014 </x-m>
       <y-m> 5.165 </y-m>
       <z-m> 0.079 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Déformation des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>pale1</object-name>
    <object-name>propblur1</object-name>
    <object-name>propdisc1</object-name>
    <object-name>pale1B</object-name>
    <object-name>propblur1B</object-name>
    <object-name>propdisc1B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep>  6.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale2</object-name>
    <object-name>propblur2</object-name>
    <object-name>propdisc2</object-name>
    <object-name>pale2B</object-name>
    <object-name>propblur2B</object-name>
    <object-name>propdisc2B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep>  6.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>  -4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale3</object-name>
    <object-name>propblur3</object-name>
    <object-name>propdisc3</object-name>
    <object-name>pale3B</object-name>
    <object-name>propblur3B</object-name>
    <object-name>propdisc3B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>   0.0 </ind>
        <dep>  -6.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>   4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale4</object-name>
    <object-name>propblur4</object-name>
    <object-name>propdisc4</object-name>
    <object-name>pale4B</object-name>
    <object-name>propblur4B</object-name>
    <object-name>propdisc4B</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry>
        <ind>   0.0 </ind>
        <dep>  -6.0 </dep>
      </entry>
      <entry>
        <ind> 180.0 </ind>
        <dep>   0.0 </dep>
      </entry>
      <entry>
        <ind> 242.0 </ind>
        <dep>   4.0 </dep>
      </entry>
    </interpolation>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Rotation du rotor -->
  <animation>
    <type>rotate</type>
    <object-name>rotorpartiel</object-name>
    <property>rotors/main/roll-deg</property>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rotorpartiel</object-name>
    <property>rotors/main/yaw-deg</property>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y>-1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rotorcomplet</object-name>
    <property>rotors/main/blade[0]/position-deg</property>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z>  1.000 </z>
    </axis>
  </animation>

</PropertyList>

