<?xml version="1.0"?>
<PropertyList>

 <path>speed.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>20.00</ind>
      <dep>45.5</dep>
    </entry>
    <entry>
      <ind>60</ind>
      <dep>122.0</dep>
    </entry>
    <entry>
      <ind>80</ind>
      <dep>180</dep>
    </entry>
    <entry>
      <ind>100</ind>
      <dep>247.0</dep>
    </entry>
    <entry>
      <ind>120</ind>
      <dep>312.2</dep>
    </entry>
    <entry>
      <ind>150</ind>
      <dep>350.0</dep>
    </entry>
  </interpolation>   
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


